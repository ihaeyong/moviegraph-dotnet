@model MovieModel

@{
    ViewData["Title"] = $"{Model.Title} - Moviegraph";
}

<h1>@Model.Title (@Model.Released)</h1>
<p class="movie-tagline">@Model.Tagline</p>

@Html.EditorFor(m => m.Stars, "Stars")

<div class="movie-cast">
    <h2>Cast</h2>
    <ul>
        @Html.DisplayFor(m => m.Actors)
    </ul>
</div>